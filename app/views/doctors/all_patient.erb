<div class="breadcrumb"> <% make_breadcrumb %> 
    <%= render_breadcrumbs %>
</div>
<table class="table table-striped table-bordered table-condensed">
    <tr><th>S.no.</th> <th>Patient Name</th></tr>
    <% @patients.each_with_index do |patient, i| %>
      <tr><td><%=  i+1 %></td>
          <td><%= link_to  patient.patient.user.first_name, patient_profile_patient_path(patient.patient.id)%> <%= link_to  patient.patient.user.last_name%></td>
      </tr>
    <% end %>
</table>
